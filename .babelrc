{
  "env": {
    "development": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        ["babel-plugin-styled-components", {
          "displayName": true,
          "ssr": true
        }]
      ]
    },
    "production": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        ["babel-plugin-styled-components", {
          "displayName": true,
          "ssr": true
        }]
      ]
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ]
    }
  },
  "plugins": [
    "transform-flow-strip-types",
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "components": "./components",
        "theme": "./theme"
      }
    }],
    ["transform-assets", {
      "extensions": ["svg", "woff2", "png", "jpg"],
      "name": "/site/assets/[sha512:hash:base64:7]/[1]",
      "regExp": ".*/assets/(.+)"
    }]
  ]
}